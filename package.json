{
  "name": "electron-simple-boilerplate",
  "version": "0.0.1",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron .",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "dev": "cross-env PORT=9000 concurrently -k -n \"main,renderer\" \"npm run dev-main\" \"npm run dev-renderer\"",
    "dev-main": "cross-env HOT=1 nodemon --watch app/main --watch app/common --exec \"electron -r babel-register ./app/main/index.js\"",
    "dev-renderer": "webpack-dev-server --config configs/webpack.renderer.config.js",
    "build": "cross-env concurrently -n \"main,renderer\" \"npm run build-main\" \"npm run build-renderer\"",
    "build-main": "webpack --config configs/webpack.main.config.js",
    "build-renderer": "webpack --config configs/webpack.renderer.config.js",
    "prod": "cross-env concurrently -n \"main,renderer\" \"npm run prod-main\" \"npm run prod-renderer\"",
    "prod-main": "webpack --config configs/webpack.main.prod.config.js",
    "prod-renderer": "webpack --config configs/webpack.renderer.prod.config.js",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.1",
    "electron": "^1.7.9",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.8.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
